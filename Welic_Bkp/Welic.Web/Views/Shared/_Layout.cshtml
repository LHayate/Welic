<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width,
    initial-scale=1.0">
    @* CSS PErsonalizado *@
    <link href="~/Content/custom.css" rel="stylesheet" />
    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css" />

    <link rel="shortcut icon" href="~/Imagens/LogoWelic12x12.png">
    <title>@ViewBag.Title</title>
    
</head>
<body>
    <!-- Modal Structure -->
    <div id="modal1" class="modal">
        <div class="modal-content">
            <h4 class="blue-text text-darken-1 center-align">Welic</h4>
            <p class="blue-text text-darken-1  accent-1 right-aligned">@ViewBag.Message</p>
        </div>
        <div class="modal-footer">
            <a href="#!" class="modal-action modal-close waves-effect waves-green btn-flat">Agree</a>
        </div>
    </div>

    <!-- Navibar-->
    <div>
        <nav class="welic z-depth-2" role="navigation">
            <div class="nav-wrapper container">
                <a id="logo-container" href="@Url.Action("Index")" class="brand-logo right"><i class="material-icons right"><img src="~/Imagens/LogoWelic32x32.png" /></i>Welic</a>

                <ul class="left hide-on-med-and-down">
                    <li class="waves-effect waves-light"><a href="@Url.Action("Index")">Inicio</a></li>
                    <li><a class="waves-effect waves-light modal-trigger " href="#modal1">Sobre o Welic</a></li>                    
                    @*<li class="waves-effect waves-light">@Html.ActionLink("Log in", "Login", "Account", routeValues: null, htmlAttributes: new { id = "loginLink" })</li>*@
                    
                </ul>



                <ul id="nav-mobile" class="side-nav">
                    <li class="waves-effect waves-light"><a href="@Url.Action("Index")">Inicio</a></li>
                    <li><a class="waves-effect waves-light modal-trigger" href="#modal1">Sobre a Welic</a></li>
                    @*<li class="waves-effect waves-light">@Html.ActionLink("Log in", "Login", "Account", routeValues: null, htmlAttributes: new { id = "loginLink" })</li>*@                    
                </ul>
                <a href="#" data-activates="nav-mobile" class="button-collapse"><i class="material-icons">menu</i></a>
            </div>
        </nav>
    </div>

    <!--Footer-->
    <div>
        @RenderBody()
        <hr />       
        <footer class="page-footer blue-grey darken-4">
            <div class="container">
                <div class="row">
                    <div class="col l3 s3 m3">
